  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch via NPM",
      "request": "launch",
      "runtimeArgs": [
        "run-script",
        "debug"
      ],
      "runtimeExecutable": "npm",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },

    {
      "type": "node",
      "request": "launch",
      "name": "mssql createSchema",
      "program": "${workspaceFolder}/test/mssql/createSchema.js",
      "runtimeArgs": ["--trace-warnings"],
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "LOG_LEVEL": "verbose"
      }
    },
    {
      "type": "node",
      "request": "launch",
      "name": "mssql dullSchema",
